<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.jogodaforca.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_testeDeTela2"
            app:destination="@id/testeDeTela2" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.jogodaforca.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <fragment
        android:id="@+id/testeDeTela2"
        android:name="com.example.jogodaforca.TesteDeTela"
        android:label="fragment_teste_de_tela"
        tools:layout="@layout/fragment_teste_de_tela" >
        <action
            android:id="@+id/action_testeDeTela2_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
</navigation>